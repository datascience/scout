{{#partial "item_list"}}
<table class="table table-striped">
	<thead>
		<tr>
			<th>Name</th>
			<th style="width: 20px">Action</th>
		</tr>
	</thead>
	<tbody>
		{{#items}}
		<tr>
			<td>{{name}}</td>
			<td><a class="btn btn-small btn-link"
				href="{{../mustacheletPath}}/browse/entity/{{id}}"><i class="glyphicon glyphicon-th-list"></i></a></td>
		</tr>
		{{/items}}
	</tbody>
</table>
{{/partial}}
{{#partial "empty_message"}}There are no defined entities.{{/partial}}
